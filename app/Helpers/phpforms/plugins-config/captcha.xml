<?xml version="1.0" encoding="UTF-8"?>
<root>
	<default>
		<includes>
			<!--
				Path to css and js files into plugins folder.
				If your files are not in plugins folder, use absolute paths.
			-->
			<css>
				<file>jquery-realperson/jquery.realperson.css</file>
			</css>
			<js>
				<file>jquery-realperson/jquery.plugin.min.js</file>
				<file>jquery-realperson/jquery.realperson.min.js</file>
			</js>
		</includes>
		<js_code>
<![CDATA[
	$("%selector%").realperson();
]]>
		</js_code>
	</default>
	<custom>
		<!--
			If you need differents includes for your custom code,
			copy the default '<includes>' structure and put the needed files into '<file>' nodes.
			You can add several files nodes if necessary.
		-->
		<js_code>
<![CDATA[
	$("%selector%").realperson({
		length: 6,
		regenerate:'Click to change',
		dot: '#'
	});
]]>
		</js_code>
	</custom>
</root>